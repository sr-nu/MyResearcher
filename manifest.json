{
  "name": "My ReSearcher",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Searches everything from the bookmarked websites",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "http://api.flickr.com/",
    "http://*.google.com",
    "http://*.google.co.in"
  ]
}
